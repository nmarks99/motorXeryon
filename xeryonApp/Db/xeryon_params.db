record(longout, "$(P)$(M):Freq1") {
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT=1))FREQUENCY1")
}
record(longin, "$(P)$(M):Freq1RBV") {
    field(DTYP, "asynInt32")
    field(SCAN, "I/O Intr")
    field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT=1))FREQUENCY1")
}

